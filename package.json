{
  "name": "acf-images-search-and-insert",
  "version": "1.1.3",
  "main": "Gruntfile.js",
  "author": "Takayuki Imanishi",
  "scripts": {
    "clean": "rm -rf dist",
    "watch": "better-npm-run watch",
    "compile": "better-npm-run compile",
    "lint": "eslint . ./",
    "lint:fix": "npm run lint -- --fix"
  },
  "betterScripts": {
    "watch": {
      "command": "node -r dotenv/config --harmony bin/watch",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "compile": {
      "command": "npm run clean && node -r dotenv/config --harmony bin/compile",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
